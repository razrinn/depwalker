{
  "name": "depwalker",
  "version": "0.2.1",
  "description": "A comprehensive TypeScript dependency analysis tool that tracks the impact of code changes, including function calls and variable usage across your codebase",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "depwalker": "dist/index.js"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "rm -rf dist && tsc && node scripts/build.js",
    "depwalker": "node dist/index.js",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "typescript",
    "dependency-analysis",
    "impact-analysis",
    "code-analysis",
    "ast",
    "git",
    "refactoring",
    "cli",
    "developer-tools",
    "function-tracking",
    "variable-tracking",
    "static-analysis"
  ],
  "author": {
    "name": "Ray Azrin Karim",
    "url": "https://github.com/razrinn"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/razrinn/depwalker.git"
  },
  "bugs": {
    "url": "https://github.com/razrinn/depwalker/issues"
  },
  "homepage": "https://github.com/razrinn/depwalker#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "commander": "^14.0.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "^24.2.1",
    "esbuild": "^0.27.2"
  }
}
